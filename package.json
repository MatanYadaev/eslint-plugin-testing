{
  "name": "eslint-plugin-testing",
  "version": "0.0.0",
  "description": "ESLint plugin for testing",
  "keywords": [
    "testing",
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "eslint-plugin-testing"
  ],
  "homepage": "https://github.com/MatanYadaev/eslint-plugin-testing#readme",
  "bugs": {
    "url": "https://github.com/MatanYadaev/eslint-plugin-testing/issues"
  },
  "repository": "MatanYadaev/eslint-plugin-testing",
  "license": "MIT",
  "author": "Matan Yadaev",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --clean --minify --dts",
    "lint:eslint-docs": "pnpm build && eslint-doc-generator --check",
    "release": "pnpm build && bumpp package.json && pnpm publish",
    "test": "vitest run",
    "test:watch": "vitest",
    "update:eslint-docs": "pnpm build && eslint-doc-generator",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^6.4.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^16.1.0",
    "@tsconfig/strictest": "^2.0.1",
    "@types/eslint": "^8.44.2",
    "@typescript-eslint/parser": "^6.4.0",
    "@typescript-eslint/rule-tester": "^6.4.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.47.0",
    "eslint-doc-generator": "^1.4.3",
    "eslint-plugin-eslint-plugin": "^5.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1"
  },
  "peerDependencies": {
    "eslint": "^7.0.0 || ^8.0.0"
  },
  "packageManager": "pnpm@8.6.3",
  "engines": {
    "node": ">=16.0.0"
  }
}
